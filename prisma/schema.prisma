generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = env("DATABASE_URL")
}

model Keep {
  id            Int      @id @default(autoincrement())
  title         String
  fabric_object Json
  width         Int
  height        Int
  svg           String   @db.LongText
  created_at    DateTime @default(now())
  updated_at    DateTime @updatedAt
}
